{
    "name": "ytc-dashboard-feature-shell",
    "$schema": "../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/ytc-dashboard/feature-shell/src",
    "prefix": "ytc",
    "tags": ["scope:ytc-dashboard", "type:feature"],
    "projectType": "library",
    "targets": {
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "jestConfig": "libs/ytc-dashboard/feature-shell/jest.config.ts",
                "passWithNoTests": true
            },
            "configurations": {
                "ci": {
                    "ci": true,
                    "codeCoverage": true
                }
            }
        },
        "lint": {
            "executor": "@nx/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": [
                    "libs/ytc-dashboard/feature-shell/**/*.ts",
                    "libs/ytc-dashboard/feature-shell/**/*.html"
                ]
            }
        }
    }
}
