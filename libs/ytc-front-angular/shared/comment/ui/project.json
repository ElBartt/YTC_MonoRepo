{
    "name": "ytc-front-angular-shared-comment-ui",
    "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/ytc-front-angular/shared/comment/ui/src",
    "prefix": "ytc",
    "tags": ["scope:ytc-front", "type:ui"],
    "projectType": "library",
    "targets": {
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "jestConfig": "libs/ytc-front-angular/shared/comment/ui/jest.config.ts",
                "passWithNoTests": true
            },
            "configurations": {
                "ci": {
                    "ci": true,
                    "codeCoverage": true
                }
            }
        },
        "lint": {
            "executor": "@nx/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": [
                    "libs/ytc-front-angular/shared/comment/ui/**/*.ts",
                    "libs/ytc-front-angular/shared/comment/ui/**/*.html"
                ]
            }
        }
    }
}
