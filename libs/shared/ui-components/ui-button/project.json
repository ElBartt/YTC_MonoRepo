{
    "name": "shared-ui-components-ui-button",
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/shared/ui-components/ui-button/src",
    "prefix": "ytc",
    "tags": ["scope:shared", "type:ui"],
    "projectType": "library",
    "targets": {
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "jestConfig": "libs/shared/ui-components/ui-button/jest.config.ts",
                "passWithNoTests": true
            },
            "configurations": {
                "ci": {
                    "ci": true,
                    "codeCoverage": true
                }
            }
        },
        "lint": {
            "executor": "@nx/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": [
                    "libs/shared/ui-components/ui-button/**/*.ts",
                    "libs/shared/ui-components/ui-button/**/*.html"
                ]
            }
        }
    }
}
